{
    "name": "fei/chat-package",
    "description": "Chat client package integration for Objective PHP applications",
    "type": "library",
    "license": "proprietary",
    "authors": [
        {
            "name": "Boris CERATI",
            "email": "bce@opcoding.eu"
        }
    ],
    "autoload": {
        "psr-4": {
            "Fei\\Service\\Chat\\Package\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "git",
            "url": "git@github.com:flash-global/chat-client.git"
        },
        {
            "type": "composer",
            "url": "https://composer.flash.global"
        }
    ],
    "require": {
        "objective-php/application": "~1.1.0",
        "fei/chat-client": "^2.1"
    },
    "require-dev": {
        "phpro/grumphp": "^0.11.1",
        "jakub-onderka/php-parallel-lint": "^0.9.2",
        "squizlabs/php_codesniffer": "^2.7",
        "sebastian/phpcpd": "^2.0",
        "phpmd/phpmd": "^2.6"
    }
}
